@charset "UTF-8";
/* UNIVERSIDADE ABERTA DO SUS - Web App - 2013/2014 */
@font-face {
	font-family: 'Helvetica Neue';
	src: local("Helvetica Neue Light"), local("HelveticaNeue-Light"), url("HelveticaNeue-Light.svg#HelveticaNeue-Light") format("svg"), url("HelveticaNeue-Light.woff") format("woff"), url("HelveticaNeue-Light.eot");
	font-weight: 300
}
@font-face {
	font-family: 'Helvetica Neue';
	src: local("Helvetica Neue Medium"), local("HelveticaNeue-Medium"), url("HelveticaNeue-Medium.svg#HelveticaNeue-Medium") format("svg"), url("HelveticaNeue-Medium.woff") format("woff"), url("HelveticaNeue-Medium.eot");
	font-weight: 500
}
@import "glyphicons.css";
* {
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-o-user-select: none;
	user-select: none;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	-khtml-tap-highlight-color: rgba(0,0,0,0);
	-moz-tap-highlight-color: rgba(0,0,0,0);
	-o-tap-highlight-color: rgba(0,0,0,0);
	tap-highlight-color: rgba(0,0,0,0);
	cursor: default;
	font-family: "Helvetica Neue", sans-serif;
	font-weight:300;
	-webkit-font-smoothing: antialiased;
	-khtml-font-smoothing: antialiased;
	-moz-font-smoothing: antialiased;
	-o-font-smoothing: antialiased;
	font-smoothing: antialiased;
}
html, body {
	height: 100% !important;
	margin: 0px;
	padding: 0px;
	-ms-touch-action: auto;
}
body, button {
	/*font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "HelveticaNeue", Helvetica, Roboto, Arial, sans-serif;*/
	font-weight: 300;
	/*-webkit-font-smoothing: antialiased;
	font-smoothing: antialiased;*/
	text-shadow: 0 1px 0 rgba(0,0,0,0.01);
	cursor: default;
}
.accessibility-reader {
	display: none;
}
#header {
	height: 60px;
	width: 100%;
	top: 0px;
	left: 0px;
	right: 0px;
	position: fixed;
	overflow: visible;
	background-color: rgba(0,94,170,0.9);
	z-index: 99;
	background-image: url(../icons/logo.svg);
	background-position: bottom center;
	background-repeat: no-repeat;
}
#header #buttons_left {
	padding: 15px 0px 0px 10px;
	color: rgba(255,255,255,1);
	width: 0px;
	overflow: visible;
	float: left;
	display: block;
}
#header #buttons_left #btn_return {
	background-image: url(../icons/arrow.svg);
	background-position: center left;
	background-repeat: no-repeat;
	padding: 12px 0px 10px 25px;
	height: 20px;
	font-size: 15px;
	display: block;
	width: 0%;
}
#header #buttons_right {
	padding: 15px 0px 0px 10px;
	color: rgba(255,255,255,1);
	display: block;
	float: right;
	width: 0px;
	overflow: visible;
	text-align: right;
}
#header #buttons_right #btn_menu {
	background-image: url(../icons/menu.svg);
	background-position: center left;
	background-repeat: no-repeat;
	background-size: contain;
	width: 50px;
	height: 40px;
	font-size: 0px;
	display: block;
	float: right;
}
#thebody {
	background-color: rgba(255,255,255,1);
	width: 100%;
	height: 100%;
	display: block;
	z-index: 1;
}
#footer {
	bottom: 0px;
	left: 0px;
	width: 100%;
	position: fixed;
	height: 30px;
	background-color: rgba(255,255,255,0.9);
	text-align: center;
	padding: 7px 0px 7px 0px;
	font-size: 15px;
	border-top: rgba(153,153,153,0.5) solid 1px;
}
#installrequired {
	height: 100%;
	width: 100%;
	top: 0px;
	left: 0px;
	right: 0px;
	position: fixed;
	overflow: visible;
	background-color: rgba(51,51,51,0.9);
	z-index: 100;
}
#installrequired #alert_install {
	width: 50%;
	margin: 0 auto;
	padding: 100px 0px 0px 0px;
	color: rgba(255,255,255,1);
	text-align: center;
	font-size: 20px;
}
#installrequired #alert_install strong {
	font-size: 60px;
	font-weight: 300;
	padding: 0px 0px 60px 0px;
	display: block;
}
h1, h2, h3, h4, h5, h6 {
	font-weight: 300;
	color: rgba(0,102,204,1);
	padding: 0px;
	margin: 0px;
}
#loading_status {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0px;
	bottom: 0px;
	left: 0px;
	right: 0px;
	z-index: 999999999999999999999999;
	background-color:rgba(255,255,255,1);
	background-position:center;
	background-repeat:no-repeat;
}
#loading_status {
	background-image:url('../icons/splash/startup-768x1004.png');
	background-size:auto 90%;
}

#mainhero {
	width: 100%;
	text-align: center;
	font-weight: 200;
	display: block;
}
#tales {
	padding: 70px 15px 0px 15px;
	height: 300px;
	display: block;
}
#tales #taleitem span {
	display: block;
	font-size: 12px;
	padding-top: 10px;
	text-align: left;
}
/* scrollbars */
.swiper-container {
	height: 100%;
	width: 100%;
}
.slide-inner {
}
.swiper-scrollbar {
	background: none;
	position: absolute;
	right: 3px;
	top: 0;
	height: 100%;
	width: 5px;
}
.swiper-scrollbar .swiper-scrollbar-drag {
	background: rgba(255,255,255,0.3);
}
/* Page effects */
.blurelement {
	/*filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale"); /* Firefox 10+, Firefox on Android */
	/*filter: gray; /* IE6-9 */
	/*-webkit-filter: grayscale(100%); /* Chrome 19+, Safari 6+, Safari 6+ iOS */
	-webkit-transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	transition: all 0.5s ease-in-out;
}
.unblurelement {
	/*filter: none;
	-webkit-filter: none;*/
	-webkit-transition: all 0.1s ease-in-out;
	-moz-transition: all 0.1s ease-in-out;
	-ms-transition: all 0.1s ease-in-out;
	-o-transition: all 0.1s ease-in-out;
	transition: all 0.1s ease-in-out;
}
/* menu */
#menu_area {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0px;
	bottom: 0px;
	left: 0px;
	right: 0px;
	z-index: 999999999999999999999999;
	background: -moz-radial-gradient(center, ellipse cover, rgba(191,191,191,0.8) 0%, rgba(191,191,191,0.5) 100%);
	background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, rgba(191,191,191,0.8)), color-stop(100%, rgba(191,191,191,0.5)));
	background: -webkit-radial-gradient(center, ellipse cover, rgba(191,191,191,0.8) 0%, rgba(191,191,191,0.5) 100%);
	background: -o-radial-gradient(center, ellipse cover, rgba(191,191,191,0.8) 0%, rgba(191,191,191,0.5) 100%);
	background: -ms-radial-gradient(center, ellipse cover, rgba(191,191,191,0.8) 0%, rgba(191,191,191,0.5) 100%);
	background: radial-gradient(ellipse at center, rgba(191,191,191,0.8) 0%, rgba(191,191,191,0.5) 100%);
 filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ccbfbfbf', endColorstr='#80bfbfbf', GradientType=1 );
}
#menu_area #the_menu {
	background-color: rgba(255,255,255,0.9);
	padding: 10px;
	-webkit-border-radius: 6px;
	border-radius: 6px;
}
#menu_area #menu_pointer {
	background-image: url(../icons/menu_point.svg);
	background-repeat: no-repeat;
	background-position: bottom right;
	height: 14px;
	opacity: 0.9;
}
#menu_area #the_menu ul {
	list-style: none;
	padding: 0px;
	margin: 0px;
}
#menu_area #the_menu ul li a {
	color: rgba(51,51,51,1);
	text-decoration: none;
}
#menu_area #the_menu ul li {
	border-bottom: rgba(153,153,153,0.3) solid 1px;
	padding: 6px 0px 6px 0px;
}
#menu_area #the_menu ul li:last-child {
	border-bottom: none;
}
#theoptions {
	display: none;
	padding-top: 60px;
	z-index: 1;
}
#options-box {
	padding: 15px;
}
#options-box h1, #options-box h2, #options-box h3 {
	color: rgba(51,51,51,1);
}
.btn {
	display: inline-block;
	padding: 8px 12px;
	margin-bottom: 0;
	font-size: 14px;
	font-weight: 500;
	line-height: 1.428571429;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
}
.btn-default {
	color: #ffffff;
	background-color: #474949;
	border-color: #474949;
}
.btn-primary {
	color: #ffffff;
	background-color: #428bca;
	border-color: #428bca;
}
.btn-success {
	color: #ffffff;
	background-color: #5cb85c;
	border-color: #5cb85c;
}
.btn-info {
	color: #ffffff;
	background-color: #5bc0de;
	border-color: #5bc0de;
}
.btn-warning {
	color: #ffffff;
	background-color: #f0ad4e;
	border-color: #f0ad4e;
}
.btn-danger {
	color: #ffffff;
	background-color: #d9534f;
	border-color: #d9534f;
}
hr {
	height: 0;
	-moz-box-sizing: content-box;
	box-sizing: content-box;
}
#updatecache {
	position: fixed;
	bottom: 0px;
	width: 100%;
	font-size: 15px;
	background-color: rgba(255,255,255,0.8);
	color: rgba(0,0,0,1);
	text-align: center;
	padding: 15px 0px 15px 0px;
}
