/* CSS Document */

body {
	margin:0px;
}

/* Inside the world of any course */
#box-startworld {
	display: block;
	padding:70px 15px 0px 15px;
}
#box-startworld #introductiontocontent {
	padding: 15px;
}
#box-startworld #introductiontocontent h1 {
	font-weight: 200;
	color: rgba(0,94,170,1)
}
#box-startworld #openworldicons {
	padding: 15px;
}
#openworldicons a span {
	font-size: 12px;
	display: block;
	opacity:0.8;
}
#openworldicons a {
	background-position:right bottom;
	background-size:contain;
	background-repeat:no-repeat;
}


#stage-world {
	width:100%;
	height:100%;
	display:block;
}
#stage-world #asidebar-world #asidebarcontent-world {
	padding:20px;
}
#stage-world #asidebar-world #asidebarcontentdivider-world {
	font-size:0px;
	height:15px;
	background-color:rgba(230,230,230,1);
}
#stage-world #asidebar-world .progress-bar {
	float: left;
	width: 0;
	height: 100%;
	font-size: 12px;
	color: #ffffff;
	text-align: center;
	background-color: #428bca;
	-webkit-transition: width 0.6s ease;
	transition: width 0.6s ease;
}
.activeaside {
	background-image: url("data:image/svg+xml;base64,");
	background-position:center left;
	background-repeat:no-repeat;
	background-size:15px;
}
#stage-world #asidebar-world .progress-bar-success {
	background-color: #5cb85c;
}
#stage-world #asidebar-world .progress {
	height: 10px;
	overflow: hidden;
	background-color: #f5f5f5;
	-webkit-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
	box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
}
#stage-world #content-world label {
	background-image: url("data:image/svg+xml;base64,");
	background-position:5px center;
	background-repeat:no-repeat;
	background-size:30px auto;
}
#stage-world #content-world input[type="radio"]:checked + label { 
	background-color:rgba(242,242,242,1);
	background-image: url("data:image/svg+xml;base64,");
}
#stage-world #content-world #questionarea-world button {
	padding:10px;
	color:rgba(255,255,255,1);
	background-color:rgba(102,102,102,1);
	border:rgba(102,102,102,1) 2px solid;
}
#stage-world #content-world #questionarea-world #buttoncheck-world {
	text-align:right;
	padding:15px 0px 0px 0px;
}
#stage-world #content-world #hipertext-world {
	display:none;
	position:relative;
	height:100%;
	width:100%;
}
#stage-world #content-world #hipertext-world #header-hipertextarea-world .nextquestion-info-world {
	background-color:transparent;
	margin:0px;
	float:right;
	padding:15px 30px 15px 15px;
	font-size:15px;
	border:none 0px;
	background-image:url("data:image/svg+xml;base64,");
	background-position:center right;
	background-repeat:no-repeat;
}

#stage-world #content-world #hipertext-world #header-hipertextarea-world .hipertext-info-world {
	background-color:transparent;
	margin:0 auto;
	height:48px;
	width:48px;
	font-size:0px;
	border:none 0px;
	background-image:url("data:image/svg+xml;base64,");
	background-position:center center;
	background-repeat:no-repeat;
}


#stage-world #content-world #hipertext-world .hipertext-html, #stage-world #content-world #hipertext-world .hipertext-info {
	color:rgba(51,51,51,1);
	line-height:150%;
}
#stage-world #content-world #hipertext-world .hipertext-html a, #stage-world #content-world #hipertext-world .hipertext-info a {
	background-color:rgba(255,204,0,0.9);
	padding:4px;
	text-decoration:none;
	color:rgba(51,51,51,1);
	cursor:pointer;
}
#stage-world #content-world #hipertext-world .hipertext-html img {
	max-width:100%;
	margin:0 auto;
}
#stage-world #content-world #hipertext-world .hipertext-html h1 a, #stage-world #content-world #hipertext-world .hipertext-html h2 a, #stage-world #content-world #hipertext-world .hipertext-html h3 a, #stage-world #content-world #hipertext-world .hipertext-html h4 a, #stage-world #content-world #hipertext-world .hipertext-html h5 a {
	background-color:transparent;
	padding:inherit;
}

#stage-world #content-world #hipertext-world .hipertext-html h1, #stage-world #content-world #hipertext-world .hipertext-html h2, #stage-world #content-world #hipertext-world .hipertext-html h3, #stage-world #content-world #hipertext-world .hipertext-html h4, #stage-world #content-world #hipertext-world .hipertext-html h5 {
	margin:10px 0px 10px 0px;
}

#stage-world #content-world #hipertext-world .hipertext-info .rightquestion-world {
	background-image: url("data:image/svg+xml;base64,");
}
#stage-world #content-world #hipertext-world .hipertext-info .wrongquestion-world {
	background-image: url("data:image/svg+xml;base64,");
}
#stage-world #content-world #hipertext-world .hipertext-info .wrongquestion-world, #stage-world #content-world #hipertext-world .hipertext-info .rightquestion-world {
	background-position:left top;
	background-repeat:no-repeat;
	background-size:contain;
	height:80px;
	width:90%;
}

.invisible-world {
	height:0px !important;
	width:0px !important;
	overflow:hidden;
}
/* colors of the world */
.color-box, .color-box:hover {
	background-color: rgba(255,102,51,1);
	color: rgba(255,255,255,1);
}

#asidebarcontent-world #btnres {
	padding:10px 10px 10px 40px;
	background-position:left center;
	background-repeat:no-repeat;
	background-size:contain;
	margin: 10px 0px;
}
.res-fluxograma {
	background-image: url(data:image/svg+xml;base64,);
}
.res-ficha {
	background-image: url(data:image/svg+xml;base64,);
}
.res-protocolo {
	background-image: url(data:image/svg+xml;base64,);
}
#fluxograma-box-center {
	position:fixed;
	bottom:0px;
	overflow:visible;
	height:0%;
	width:100%;
	z-index:10000000000000000000000000000000000;
	-webkit-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	-ms-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
	opacity:0;
}
#fluxograma-box {
	height:100%;
	margin:0 auto;
	width:95%;
	max-width:1000px;
	background-color:rgba(255,255,255,0.9);
	overflow:hidden;
	display:block;
	-webkit-box-shadow:  0px 0px 100px 30px rgba(0, 0, 0, 0.5);
    box-shadow:  0px 0px 100px 30px rgba(0, 0, 0, 0.5);
	-webkit-border-radius: 5px 5px 0px 0px;
    border-radius: 5px 5px 0px 0px;
}
#fluxograma-box svg {
	width:100%;
}
#fluxograma-box #fluxograma-header {
	background-color:rgba(51,51,51,0.8);
	color:#FFF;
	height:40px;
	display:block;
	width:100%;
}
#fluxograma-box #fluxograma-header button {
	background-color:transparent;
	color:#FFF;
	padding:10px;
	font-size:15px;
	float:right;
	margin:0px;
	border:none;
}
#fluxograma-box #fluxograma-header div {
	font-size:15px;
	margin:10px;
	float:left; 
}
#fluxograma-box #fluxograma-codigo {
	overflow:auto;
}